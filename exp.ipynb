    "import numpy as np\n",
    "\n",
    "num_epochs = 20\n",
    "step = num_epochs // 10\n",
    "for epoch in range(num_epochs):\n",
    "    if epoch % step == 0:\n",
    "        print(f\"{epoch}: True\")\n",
    "    else:\n",
    "        print(f\"{epoch}: False\")"
